        </patterns>
    </page-filter-snat-api>
    <page-firewall-nat-npt>
        <name>Firewall: NAT: NPT</name>
        <patterns>
            <pattern>ui/firewall/npt/*</pattern>
            <pattern>api/firewall/npt/*</pattern>
        </patterns>
    </page-firewall-nat-npt>
